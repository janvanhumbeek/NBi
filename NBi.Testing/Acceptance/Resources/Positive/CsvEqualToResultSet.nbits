    </system-under-test>
    <assert>
      <equal-to>
        <column index="0" role="key" type="text"/>
        <column index="1" role="value" type="numeric"/>
        <result-set>
          <row>
            <cell>CY 2005</cell>
            <cell>366</cell>
          </row>
          <row>
            <cell>CY 2006</cell>
            <cell>1015</cell>
          </row>
          <row>
            <cell>CY 2007</cell>
            <cell>1521</cell>
          </row>
          <row>
            <cell>CY 2008</cell>
