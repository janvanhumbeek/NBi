<?xml version="1.0" encoding="utf-8"?>
<testSuite name="The TestSuite" xmlns="http://NBi/TestSuite">
  <test name="Alteration has a hold" uid="0000">
    <system-under-test>
      <resultSet>
        <query>
          <report path="/path/" name="MyReport" dataset="MyDataSet"/>
        </query>
        <alteration>
          <hold>
            <column identifier="#0"/>
            <column identifier="Foo"/>
          </hold>
        </alteration>
      </resultSet>
    </system-under-test>
    <assert>
      <equalTo>
        <resultSet file="..\Csv\ResellerOrderCountByYearBefore2006.csv"/>
      </equalTo>
    </assert>
  </test>
  <test name="Alteration has a converter" uid="0007">
    <system-under-test>
      <resultSet>
        <query>
          <report path="/path/" name="MyReport" dataset="MyDataSet"/>
        </query>
        <alteration>
          <remove>
            <column identifier="#0"/>
            <column identifier="Foo"/>
          </remove>
        </alteration>
      </resultSet>
    </system-under-test>
    <assert>
      <equalTo>
        <resultSet file="..\Csv\ResellerOrderCountByYearBefore2006.csv"/>
      </equalTo>
    </assert>
  </test>
</testSuite>