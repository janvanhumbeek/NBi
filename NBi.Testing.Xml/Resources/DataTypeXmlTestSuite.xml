<?xml version="1.0" encoding="utf-8"?>
<testSuite name="The TestSuite" xmlns="http://NBi/TestSuite">
	<test name="Column 'My Column' is of type 'varchar(50)'">
		<system-under-test>
			<data-type>
				<column caption="column" table="table" perspective="dwh" connectionString="ConnectionString"/>
			</data-type>
		</system-under-test>
		<assert>
      <is>
        varchar(50)
      </is>
		</assert>
	</test>
</testSuite>