      <execution>
        <query>select * from table</query>
      </execution>
    </system-under-test>
    <assert>
      <equalTo>
        <xml-source>
          <file>Myfile.csv</file>
          <xpath>
            <from>//Path</from>
            <select attribute="Id">//Path/Item</select>
            <select>//Path/Item/SubItem</select>
          </xpath>
        </xml-source>
      </equalTo>
    </assert>
	</test>
